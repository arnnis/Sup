{
  "name": "Sup",
  "version": "0.2.5",
  "description": "A Slack client with WhatsApp like UI",
  "author": {
    "name": "Alireza Rezania",
    "email": "alirezarzna@gmail.com"
  },
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "npx jetifier && cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "cross-env NODE_ENV=development webpack-dev-server --config ./web/webpack.config.js --color --progress",
    "web:release": "cross-env NODE_ENV=production webpack --config ./web/webpack.config.js --color --progress",
    "desktop": "cross-env NODE_ENV=development webpack-dev-server --config ./desktop/webpack.config.renderer.dev.js --color --progress",
    "desktop:release": "yarn web:release && node ./desktop/build.js",
    "test": "jest",
    "lint": "eslint .",
    "predeploy": "yarn run web:release",
    "deploy": "gh-pages -d web/build",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.4.0",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@welldone-software/why-did-you-render": "^3.3.8",
    "bytes": "^3.1.0",
    "dayjs": "^1.8.16",
    "electron-context-menu": "^0.15.0",
    "electron-debug": "^3.0.0",
    "electron-dl": "^3.0.0",
    "electron-store": "^4.0.0",
    "electron-unhandled": "^3.0.0",
    "electron-updater": "^4.0.6",
    "electron-util": "^0.12.0",
    "emoji-datasource": "^5.0.1",
    "emoji-js": "^3.5.0",
    "emoji-mart": "^2.11.2",
    "emoji-mart-native": "https://github.com/arnnis/emoji-mart-native.git",
    "expo": "^35.0.0",
    "expo-av": "^8.0.0",
    "expo-location": "^8.0.0",
    "expo-video-thumbnails": "^3.0.0",
    "howler": "^2.1.3",
    "lodash": "^4.17.15",
    "memoize-one": "^5.1.1",
    "modal-enhanced-react-native-web": "^0.2.0",
    "polished": "^3.4.1",
    "react": "16.9.0",
    "react-contextmenu": "^2.13.0",
    "react-dom": "16.9.0",
    "react-logger": "^1.1.0",
    "react-native": "0.61.2",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-ellipsis": "^2.0.0",
    "react-native-document-picker": "^3.2.4",
    "react-native-emoji-input": "^1.1.9",
    "react-native-emoji-selector": "^0.1.7",
    "react-native-fast-image": "^7.0.2",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-gifted-chat": "^0.11.0",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-match-media-polyfill": "^1.0.3",
    "react-native-paper": "^2.16.0",
    "react-native-parsed-text": "taskrabbit/react-native-parsed-text",
    "react-native-reanimated": "kmagiera/react-native-reanimated",
    "react-native-responsive-ui": "^2.1.1",
    "react-native-screens": "^2.0.0-beta.13",
    "react-native-svg": "^9.11.1",
    "react-native-tab-view": "^2.10.0",
    "react-native-unimodules": "^0.7.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-stack": "^1.9.4",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.1.1",
    "react-responsive": "^8.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typesafe-actions": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.1.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "@svgr/webpack": "^4.3.3",
    "@types/emoji-mart": "^2.11.1",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.4",
    "@types/react-native": "^0.60.20",
    "@types/react-redux": "^7.1.4",
    "@types/react-responsive": "^8.0.2",
    "@types/react-test-renderer": "^16.9.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-native-web": "^0.11.7",
    "babel-preset-expo": "^7.1.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^6.0.7",
    "electron-builder": "^21.2.0",
    "eslint": "^6.5.1",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "np": "^5.0.3",
    "react-dropzone": "^10.2.1",
    "react-native-svg-transformer": "^0.13.0",
    "react-native-version": "^3.2.0",
    "react-refresh": "^0.7.1",
    "react-test-renderer": "16.9.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.1.3",
    "typescript": "^3.7.0-beta",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.0.0",
    "xo": "^0.24.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "@react-native-community"
  },
  "prettier": {
    "printWidth": 100,
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-refresh/babel"
        ]
      }
    }
  },
  "build": {
    "appId": "com.arnnis.sup",
    "files": [
      "!node_modules/*/**"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "../desktop/dist"
    },
    "publish": null
  },
  "react-native": {
    "electron": false,
    "electron-dl": false
  },
  "browser": {
    "electron": false,
    "electron-dl": false
  }
}
